{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-tagsinput@0.8.0",
      "destination": "wwwroot/lib/bootstrap-tagsinput/"
    },
    {
      "library": "simplemde@1.11.2",
      "destination": "wwwroot/lib/simplemde/"
    },
    {
      "library": "font-awesome@6.2.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "highlight.js@11.6.0",
      "destination": "wwwroot/lib/highlight.js/"
    },
    {
      "library": "twitter-bootstrap@5.2.2",
      "destination": "wwwroot/lib/twitter-bootstrap/"
    },
    {
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap-dark-5@0.1.3",
      "destination": "wwwroot/lib/bootstrap-dark-5/",
      "files": [
        "dist/css/bootstrap-dark.min.css",
        "dist/css/bootstrap-night.min.css"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "codemirror-typo@1.0.1",
      "destination": "wwwroot/lib/codemirror-typo/",
      "files": [
        "lib/codemirror-typo.min.css",
        "lib/codemirror-typo.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "dictionary-de@2.0.4",
      "destination": "wwwroot/lib/dictionary-de/",
      "files": [
        "index.aff",
        "index.dic"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "prism@9000.0.1",
      "destination": "wwwroot/lib/prism/"
    },
    {
      "provider": "jsdelivr",
      "library": "@toast-ui/editor@3.1.7",
      "destination": "wwwroot/lib/toast-ui/editor/",
      "files": [
        "dist/toastui-editor.min.css",
        "dist/toastui-editor.min.js",
        "dist/i18n/de-de.min.js",
        "dist/theme/toastui-editor-dark.min.css"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@toast-ui/editor-plugin-code-syntax-highlight@3.0.0",
      "destination": "wwwroot/lib/toast-ui/editor-plugin-code-syntax-highlight/",
      "files": [
        "dist/toastui-editor-plugin-code-syntax-highlight-all.min.js",
        "dist/toastui-editor-plugin-code-syntax-highlight.min.css"
      ]
    }
  ]
}